# Vectra AI Platform Configuration
VECTRA_BASE_URL=https://123456789.ab1.portal.vectra.ai
VECTRA_CLIENT_ID=<your-client-id>
VECTRA_CLIENT_SECRET=<your-client-secret>

# Optional: API Version (defaults to v3.4)
VECTRA_API_VERSION=v3.4

# Optional: Custom OAuth endpoint (if different from default)
# VECTRA_OAUTH_TOKEN_URL=https://123456789.ab1.portal.vectra.ai/oauth/token

# Optional: Request timeout in seconds (defaults to 30)
VECTRA_REQUEST_TIMEOUT=30

# Optional: Rate limiting settings
VECTRA_RATE_LIMIT_REQUESTS=100
VECTRA_RATE_LIMIT_PERIOD=60

# Optional: Cache settings (in seconds)
VECTRA_CACHE_TTL=300

# Optional: Logging configuration
LOG_LEVEL=INFO
LOG_FORMAT=json

# Optional: Enable development mode features
DEV_MODE=false